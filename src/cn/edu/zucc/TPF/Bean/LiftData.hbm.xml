<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

 <hibernate-mapping>
  <class name="cn.edu.zucc.TPF.Bean.LiftDataBean"  table="tbl_liftdata">
      <id name="dataid" column="dataid" type="java.lang.Integer" unsaved-value="null">
        <generator class="increment"></generator>
      </id>     
      <property name="liftid" column="liftid" type="java.lang.String"/>
      <property name="accx" column="accx" type="java.lang.Float"/>
      <property name="accy" column="accy" type="java.lang.Float"/>
      <property name="accz" column="accz" type="java.lang.Float"/>
      <property name="rotatex" column="rotatex" type="java.lang.Float"/>
      <property name="rotatey" column="rotatey" type="java.lang.Float"/>
      <property name="rotatez" column="rotatez" type="java.lang.Float"/>
      <property name="recordtime" column="recordtime" type="java.sql.Timestamp"/>
      
  </class>  
</hibernate-mapping>