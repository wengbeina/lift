<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

 <hibernate-mapping>
  <class name="cn.edu.zucc.TPF.Bean.LogBean"  table="tbl_log">
      <id name="id" column="id" type="java.lang.Integer" unsaved-value="null">
        <generator class="increment"></generator>
      </id>     
      <property name="operaterid" column="operaterid" type="java.lang.String"/>
      <property name="operateType" column="operateType" type="java.lang.String"/>
      <property name="logtime" column="logtime" type="java.sql.Timestamp"/>
      
  </class>  
</hibernate-mapping>